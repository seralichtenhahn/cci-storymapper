{
  "name": "cci-storymapper",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"yarn run dev:css\" \"yarn run dev:frontend\" \"yarn run dev:api\"",
    "dev:frontend": "vite",
    "dev:css": "tailwindcss -i ./src/assets/tailwind.css -o ./src/assets/main.css --watch",
    "dev:api": "FLASK_ENV=development flask run --port=4000",
    "build": "yarn run build:css && vite build",
    "build:css": "NODE_ENV=production tailwindcss -i ./src/assets/tailwind.css -o ./src/assets/main.css -m",
    "preview": "vite preview"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@math.gl/web-mercator": "^3.6.0",
    "@tippyjs/react": "^4.2.6",
    "clsx": "^1.1.1",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.0.15",
    "slate": "^0.81.0",
    "slate-react": "^0.81.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.13",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "tailwindcss": "^3.1.3",
    "vite": "^2.9.12"
  },
  "engines": {
    "node": "16.x"
  }
}
